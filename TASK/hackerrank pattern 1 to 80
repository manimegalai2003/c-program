9.
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    for (int i=1;i<=n;i++)
    {
        for(int j=n;j>=n-i+1;j--)
        {
            printf("%d",j);
        }
        printf("\n");
    }
    
    
    return 0;
}


