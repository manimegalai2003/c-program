1.
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d", &n);
    
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            printf("*");
        }
    printf("\n");
}
    
    return 0;
}


2.
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n,m;
    scanf("%d %d", &n,&m);
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


