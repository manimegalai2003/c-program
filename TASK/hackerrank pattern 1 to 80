1. Write a program to accept the n value and display the below pattern
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d", &n);
    
    for(int i=0;i<n;i++){
        for(int j=0;j<n;j++){
            printf("*");
        }
    printf("\n");
}
    
    return 0;
}


2.Write a program to accept the n value and display the below pattern
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n,m;
    scanf("%d %d", &n,&m);
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


3.Write a program to accept the n value and display the below pattern
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    for (int i=1;i<=n;i++){
        for (int j=1;j<=i;j++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


4.Write a program to accept the n value and display the below pattern
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    for(int i=n;i>=1;i--){
        for (int j=1;j<=i;j++){
            printf("*");
        }
        printf("\n");
    }
    return 0;
}


5.Write a program to accept the n value and display the below pattern
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    for (int i=1;i<=n;i++){
        for (int j=1;j<=i;j++){
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}

